{
  "name": "invest-electron",
  "productName": "invest-electron",
  "version": "0.1.0-alpha",
  "description": "My Electron application description",
  "main": "src/main.js",
  "homepage": "./",
  "scripts": {
    "start": "cross-env FLASK_APP=src/server.py npx electron . --dev",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps",
    "build": "npx ./build.js",
    "dist": "electron-builder -c.extraMetadata.main=build/main.js",
    "pack": "electron-builder --dir -c.extraMetadata.main=build/main.js",
    "prebuilt-invest": "npx ./scripts/get_invest_binaries_gcs.js"
  },
  "build": {
    "asarUnpack": [
      "build/invest/"
    ],
    "appId": "NaturalCapitalProject.Invest.Desktop",
    "productName": "InVEST Desktop",
    "artifactName": "invest-desktop_${os}_${arch}_${version}.${ext}",
    "mac": {
      "category": "public.app-category.business",
      "icon": "build/icon.png",
      "target": "zip"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "build/icon.png",
      "category": "Science"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "jest-environment-jsdom-sixteen"
  },
  "invest" : {
    "hostname": "https://storage.googleapis.com/",
    "bucket": "natcap-dev-build-artifacts",
    "fork": "invest/davemfish",
    "version": "3.8.0.post632+g7a58318e/"
  },
  "keywords": [],
  "author": "dmf",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "chroma-js": "^2.1.0",
    "cross-env": "^5.2.1",
    "esm": "^3.2.25",
    "font-awesome": "^4.7.0",
    "glob": "^7.1.6",
    "google-palette": "^1.1.0",
    "leaflet": "^1.6.0",
    "node-fetch": "^2.6.0",
    "papaparse": "^5.1.1",
    "prop-types": "^15.7.2",
    "radium": "^0.25.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.1",
    "react-dom": "^16.12.0",
    "react-easy-chart": "^1.0.0",
    "react-hot-loader": "^3.1.3",
    "react-lazylog": "^4.5.1",
    "react-leaflet": "^2.6.1",
    "react-leaflet-choropleth": "^2.0.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "tail": "^2.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.1",
    "@babel/runtime": "^7.10.2",
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@jest-runner/electron": "^2.0.3",
    "@testing-library/dom": "^7.5.3",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "dotenv": "^8.2.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "electron-devtools-installer": "^2.2.4",
    "jest": "^26.0.1",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "react-test-renderer": "^16.12.0",
    "sinon": "^9.0.1",
    "spectron": "^10.0.0",
    "winston": "^3.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
